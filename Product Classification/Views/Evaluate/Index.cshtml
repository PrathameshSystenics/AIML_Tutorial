@{

    ViewData["Title"] = "Eval";
    Dictionary<string, string> ModelNameWithValue = ViewBag.ModelNameWithValue;
}

<div class="mt-3">
    <h3>Evaluation</h3>

    <div class="mt-3">
        <partial name="ModelSelectors" model="ModelNameWithValue"></partial>
    </div>
    <div>
        <h3>Evaluation Result : </h3>
        <div id="eval-result">
        </div>
    </div>
    <div class="d-grid">
        <button class="btn btn-success" id="evaluate">Evaluate</button>
    </div>
</div>
@section scripts{
    <script src="~/js/Evaluate.js"></script>
}